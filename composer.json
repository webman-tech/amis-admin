{
  "name": "webman-tech/amis-admin",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin webman-tech/amis-admin",
  "autoload": {
    "psr-4": {
      "WebmanTech\\AmisAdmin\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "workerman/webman-framework": "^1.5",
    "illuminate/database": "^8.83",
    "illuminate/pagination": "^8.83",
    "illuminate/validation": "^8.83",
    "webman-tech/polyfill": "^1.0",
    "pestphp/pest": "^1.23",
    "symfony/var-dumper": "^5.4"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
